{
  "name": "bigassfans",
  "version": "0.1.0",
  "description": "A library for discovering and controlling BigAssFans",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=unrecognizedEnum=false --ts_proto_out=./src ./proto/fan.proto",
    "build": "npm run generate && npx tsc"
  },
  "keywords": [
    "BigAssFans",
    "Haiku",
    "Fan"
  ],
  "author": "Matthew Lyon",
  "license": "MIT",
  "dependencies": {
    "node-dns-sd": "^1.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "ts-proto": "^1.167.8",
    "@types/node": "^20.11.24",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.3.3"
  }
}
